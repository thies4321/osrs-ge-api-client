{
    "name": "thies4321/osrs-ge-api-client",
    "type": "standard",
    "description": "Oldschool Runescape Grand Exchange api client written in PHP",
    "homepage": "https://github.com/thies4321/osrs-ge-api-client",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OsrsGeApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OsrsGeApiClient\\": "tests/"
        }
    },
    "bin": ["bin/osrs-ge-api-client"],
    "require": {
        "php": "^8.2",
        "symfony/http-client": "^6.2",
        "symfony/yaml": "^6.2",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "psy/psysh": "@stable",
        "phpstan/phpstan": "^1.10"
    }
}
